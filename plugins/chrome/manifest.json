{
  "name": "Linky",
  "version": "0.6",
  "permissions": ["webNavigation", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/olink16.png",
      "32": "images/olink32.png",
      "48": "images/olink48.png",
      "128": "images/olink128.png"
    }
  },
  "icons": {
    "16": "images/olink16.png",
    "32": "images/olink32.png",
    "48": "images/olink48.png",
    "128": "images/olink128.png"
  },
  "manifest_version": 2
}
